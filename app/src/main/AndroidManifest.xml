<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dev.docreconciliationapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddPatientActivity"
            android:label="Add Patient" />
        <activity
            android:name=".PatientListActivity"
            android:label="Patient List" />
        <activity
            android:name=".SignUpActivity"
            android:label="Sign Up" />
        <activity
            android:name=".LogInActivity"
            android:label="Log In" />
        <activity android:name=".SelectionActivity" />
        <activity android:label="Reset Password" android:name=".ResetPasswordActivity"></activity>
    </application>

</manifest>